{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "MaxLogSize": 1000
  },
  "AllowedHosts": "*",
  "ModelConfiguration": {
    "ModelPath": "models/model.onnx",
    "TokenizerPath": "models/tokenizer.json",
    "TokenizerServiceUrl": "http://localhost:8000"
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  }
}
